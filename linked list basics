#include "stdio.h"
#include "stdlib.h"
struct node{
  int val;
  struct node *next;
};
void display(struct node *head){
  while(head!=NULL){
    printf("->%d",head->val);
    head=head->next;
  }
}

void insert(struct node **head,int n){
  struct node *ne = (struct node*)malloc(sizeof(struct node));
  struct node *last = *head;
  ne->val = n;
  ne->next=NULL;
  if(*head==NULL){
    *head = ne;
    return;
  }
 while(last->next!=NULL){
   last=last->next;
 }
 last->next=ne;
 return;
}

void bpush(struct node **head,int n){
  struct node *new_ = (struct node*)malloc(sizeof(struct node));
  new_->val = n;
  new_->next=(*head);
  (*head)=new_;
}

void inlast(struct node *head,int n){
  struct node *node_ = (struct node*)malloc(sizeof(struct node));
  if(head==NULL){
    return;
  }
  while(head->next!=NULL){
    head=head->next;
  }
  node_->val = n;
  node_->next = head->next;
  head->next=node_;
}

int main(void) {
  int no,value,i;
  printf("Enter the value to be inserted");
  struct node *head=NULL;
  for(i=0;i<2;i++){
    scanf("%d",&value);
    insert(&head,value);
  }
  display(head);
  bpush(&head,100);
  printf("\n");
  display(head);
  printf("\n");
  inlast(head->next,420);
  display(head);
   bpush(&head,999);
  printf("\n");
  display(head);
  return 0;
}
