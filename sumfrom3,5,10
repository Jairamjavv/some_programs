import java.io.*;
import java.util.*;
import java.lang.*;

public class Main
{
    static int Sum(int c,int s,int n){
        if(s == n){
            return 1;
        }else if(s<n){
            c += Sum(c,s+3,n)+Sum(c,s+5,n)+Sum(c,s+10,n);
        }
        return c;
    }
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int n = 20;
		int c=0,sum=0;
		
		System.out.println(Sum(c,sum,n));
	}
}
