import java.util.*;
import java.io.*;
import java.lang.*;

public class Main
{
	public static void main(String[] args) {
		LinkedList<Integer> l = new LinkedList<>();
		l.add(10);
		l.add(20);
		l.add(30);
		l.add(40);
		l.addLast(50);
		l.addFirst(5);
		l.add(2,100);
		
		System.out.println(l.get(3));
		
		boolean available = l.contains(100); 
		System.out.println(available);
		System.out.println(l);
	}
}
